  <PropertyGroup>
    <BundledPTVSDVersion>4.0.0a6</BundledPTVSDVersion>
  </PropertyGroup>
  <Target Name="_GatherPtvsd" BeforeTargets="_IncludePtvsd" Condition="!Exists('$(IntermediateOutputPath)Packages\ptvsd\__init__.py')">
    <PropertyGroup>
      <PipCommand>"$(PackagesPath)python\tools\python.exe" -m pip install ptvsd==$(BundledPTVSDVersion)</PipCommand>
      <PipCommand>$(PipCommand) --no-compile --upgrade --target "$(IntermediateOutputPath)Packages"</PipCommand>
    </PropertyGroup>
    <Exec Command="$(PipCommand)" />
  </Target>
  <Target Name="_IncludePtvsd" BeforeTargets="AssignTargetPaths;GetVSIXSourceItems">
    <ItemGroup>
      <PtvsdFiles Include="$(IntermediateOutputPath)Packages\ptvsd\**\*" />
      <PtvsdFiles>
        <IncludeInVSIX>true</IncludeInVSIX>
        <Link>Packages\ptvsd\%(RecursiveDir)%(Filename)%(Extension)</Link>
        <VSIXSubPath>Packages\ptvsd\%(RecursiveDir)</VSIXSubPath>
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </PtvsdFiles>
      <Content Include="@(PtvsdFiles)" />
      <FileWrites Include="@(PtvsdFiles)" />
    </ItemGroup>
  </Target>
  <Target Name="_UpdatePtvsdVersion" AfterTargets="PrepareForBuild" Condition="'$(Language)' == 'C#'" Inputs="$(BuildRoot)\Python\Product\Debugger\PtvsdVersion.cs" Outputs="$(IntermediateOutputPath)\PtvsdVersion.g.cs">
    <ItemGroup>
      <_PtvsdVersionOldItems Include="@(Compile)" Condition="'%(Filename)' == 'PtvsdVersion'" />
      <Compile Remove="@(_PtvsdVersionOldItems)" />
      <_PtvsdVersionNewItem Include="$(IntermediateOutputPath)\PtvsdVersion.g.cs" />
    </ItemGroup>
    <PropertyGroup>
      <OriginalPtvsdVersionText>$([System.IO.File]::ReadAllText('$(BuildRoot)\Python\Product\Debugger\PtvsdVersion.cs'))</OriginalPtvsdVersionText>
      <PtvsdVersionText>$(OriginalPtvsdVersionText.Replace(` Version = "1.0.0"`, ` Version = "$(BundledPTVSDVersion)"`))</PtvsdVersionText>
    </PropertyGroup>
    <WriteLinesToFile File="@(_PtvsdVersionNewItem)" Lines="$(PtvsdVersionText)" Overwrite="true" />
    <ItemGroup>
      <Compile Include="@(_PtvsdVersionNewItem)">
        <Link>PtvsdVersion.cs</Link>
      </Compile>
      <FileWrites Include="@(_PtvsdVersionNewItem)" />
    </ItemGroup>
  </Target>
